{
  "name": "@gazegroup/javascript-sdk",
  "version": "0.0.1",
  "description": "JavaScript software development kit for developing applications for Gaze.",
  "type": "module",
  "main": "./dist/app.js",
  "scripts": {
    "build": "tsc -b --clean && tsc -b",
    "start": "tsc && node --env-file=.env --trace-deprecation dist/app.js"
  },
  "imports": {
    "#utilities/*": "./dist/utilities/*",
    "#resources/*": "./dist/resources/*"
  },
  "dependencies": {
    "express": "^5.1.0",
    "pg": "^8.16.3"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/pg": "^8.15.5"
  }
}
